<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
	<group id="default">
		<job instance="Monogo\Weather\Cron\Synchronise" method="execute" name="weather_sync">
            <!-- CRON time can configured through admin-->
			<config_path>weather/general/cron_timing</config_path>
		</job>
	</group>
</config>
